#include <iostream>
using namespace std;

void swap(int *a, int *b) {
    int t = *a;
    *a = *b;
    *b = t;
}

int main() {
    int x = 10, y = 20;

    cout << "b swap: x = " << x << "y = " << y << endl;
    swap(&x, &y);

    cout << "af swap: x = " << x << ", y = " << y << endl;

    return 0;
}


